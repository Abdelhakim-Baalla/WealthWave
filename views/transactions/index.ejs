<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title><%= title %></title>
  </head>
  <body>
    <% if (typeof error !== 'undefined' && error) { %>
    <div class="error-message"><%= error %></div>
    <% } %> <% if (typeof message !== 'undefined' && message) { %>
    <div class="message-message"><%= message %></div>
    <% } %>

    <h1>Transactions</h1>
    <div>
      <table></table>
      <% for(let transaction of toutTransactions) { %>
      <div>
        <div>
          <%= transaction.date %>
        </div>
        <div>
          <div><%= transaction.categorie.nom %></div>
          <div>
            <%= typeof transaction.note !== 'undefined' ? transaction.note : ''
            %>
          </div>
          <div>
            <h3><%= transaction.type %></h3>
          </div>
          <div>
            <i><%= transaction.prix %></i>
          </div>
        </div>
      </div>

      <% } %>
    </div>
  </body>
</html>
